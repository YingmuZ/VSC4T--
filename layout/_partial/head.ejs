  <link
    href="https://fonts.googleapis.com/css2?family=JetBrains+Mono:wght@400;700&display=swap"
    rel="stylesheet"
  />

  <%- partial('_partial/custom_head') %>
  <script src="<%- url_for('js/explorer.js') %>"></script>
  <script src="<%- url_for('js/code-copy.js') %>"></script>
  <script src="<%- url_for('js/code-enhance.js') %>"></script>
